.filter_content {
    background-color: #FFF;
    padding: 5px;
    overflow: auto;
}

.filter table {
/*    width: 100%;*/
    border-collapse: collapse;
/*    margin: 5px;*/
margin: 0 auto;

}

.filter table td {
    vertical-align: top;
}

.filter_content table td {
/*    border: 1px solid black;*/
   
