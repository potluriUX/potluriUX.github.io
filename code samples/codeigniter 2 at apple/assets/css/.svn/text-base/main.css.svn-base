.segmented_control {
    position: relative;
    padding-top: 10px;
    width: 300px;
    margin: 0 auto;
    background-color: transparent;
    -webkit-user-select:none;
    -moz-user-select:none;
}

.segmented_control ul {
    list-style: none;
    background-color: #3B3B3B;
}

.segmented_control ul li {
    height: 25px;
    line-height: 25px;
    vertical-align: middle;
    float: left;
    border-top: 1px solid #555555;
    border-right: 1px solid #555555;
    border-bottom: 1px solid #555555;
    padding-left: 5px;
    padding-right: 5px;
    background-color: #DDD;
    -webkit-background-clip: padding-box;
}

.segmented_control ul:after{
    clear:both;
}

.segmented_control ul li:first-child {
    border-left: 1px solid #555555;
    -moz-border-radius-topleft: 4px;
	-webkit-border-top-left-radius: 4px;
	-moz-border-radius-bottomleft: 4px;
	-webkit-border-bottom-left-radius: 4px;

}

.segmented_control ul li:last-child {
	-moz-border-radius-topright: 4px;
	-webkit-border-top-right-radius: 4px;
	-moz-border-radius-bottomright: 4px;
	-webkit-border-bottom-right-radius: 4px;
    border-right: 1px solid #555555;
}

.segmented_control ul li.selected {
    background-color: #c8ff44;
}

.segmented_control ul li:hover {
    cursor: pointer;
}

.data_cell {
    text-align: right;
}

.data_cell:hover {
    cursor:pointer;
}

.carrier_info {
