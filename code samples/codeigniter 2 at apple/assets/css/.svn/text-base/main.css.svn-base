}

.report_results {
    position: relative;
}

.report_toolbar {
}

.carrier_content {
    padding: 5px;
    background-color: #FFF;
    border-bottom-left-radius: 6px 6px;
    border-bottom-right-radius: 6px 6px;
    border-top: 1px solid #999;
    background-color: #eee;
}

.carrier_title {
    position: relative;
	-moz-border-radius-topright: 6px;
	-webkit-border-top-right-radius: 6px;
	-moz-border-radius-topleft: 6px;
	-webkit-border-top-left-radius: 6px;
    height: 20px;
    line-height: 20px;
    vertical-align: middle;
    background-color: #ccc;
    padding-left: 5px;
/*    border-bottom: 1px solid #999;*/
    font-weight: bold;
}
.carrier_title:hover {
    cursor: pointer;
}

.carrier_detail table {
    border-collapse: collapse;
    width: 100%;
}
.carrier_detail table th:first-child {
    background-color: #eee;
}
.carrier_detail table th {
    font-size: 10px;
    background-color: #fff;
}
.carrier_detail table th, .carrier_detail table td {
    border: 1px solid #ccc;
    padding: 2px;
}
.carrier_detail table th:first-child {
    border: none;
}

.carrier_detail table tbody tr:nth-child(odd) {
    background-color: #FFF;
}

.carrier_detail table tbody tr:nth-child(even) {
    background-color: #d1dff1;
}

.carrier_detail table tbody tr.total_row {
    background-color: #ddd;
    font-weight: bold;

}

.carrier_detail table tfoot tr {
    background-color: #aaa;
    font-weight: bold;
}

.carrier_detail table tfoot tr td:first-child, .carrier_detail table tr.total_row td:first-child {
    text-align: right;
}

