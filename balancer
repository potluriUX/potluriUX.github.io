	function loaderTop()
	{
		disableTest();
		document.getElementById("overlayLoader").style.top = (document.getElementById('dispCalcConts').offsetTop+(document.getElementById('dispCalcConts').offsetHeight/4))+"px";
	}
	document.load = loaderTop();
	function disableTest()
	{
		document.getElementById("dispCalcConts").disabled = true;
		var nodes = document.getElementById("dispCalcConts").getElementsByTagName('*');
		for(var i = 0; i < nodes.length; i++)
		{
		    nodes[i].disabled = true;
		}
	}
